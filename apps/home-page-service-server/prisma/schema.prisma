datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ServiceTime {
  createdAt DateTime            @default(now())
  day       EnumServiceTimeDay?
  id        String              @id @default(cuid())
  time      DateTime?
  updatedAt DateTime            @updatedAt
}

model WelcomeMessage {
  createdAt   DateTime @default(now())
  id          String   @id @default(cuid())
  messageText String?
  updatedAt   DateTime @updatedAt
}

model User {
  createdAt DateTime @default(now())
  email     String?  @unique
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     Json
  updatedAt DateTime @updatedAt
  username  String   @unique
}

enum EnumServiceTimeDay {
  Option1
}
